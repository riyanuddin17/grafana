[Unit]
Description=Prometheus Server
Documentation=https://prometheus.io/docs/introduction/overview/
After-network-omline.target
[Service]
User=root
Restart=on-failure
ExecStart=/home/ubuntu/prometheus-2.49.1.linux-amd64/prometheus --config.file=/home/ubuntu/prometheus-2.49.1.linux-amd64/prometheus.yml
[Install]
WantedBy=multi-user.target
